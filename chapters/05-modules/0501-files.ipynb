    "text = \"\"\"Hello, this is the first line of a new text file.\n",
    "This is the second line.\"\"\"\n",
    "\n",
    "with open('text_file.txt', 'w') as f:\n",
    "    f.write(text)\n",
    "\n",
    "with open('text_file.txt') as f:\n",
    "    file_content = f.read()       ### file object\n",
    "    print(file_content)"
